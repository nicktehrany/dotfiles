[Appearance]
ColorScheme=DarkPastels
Font=JetBrains Mono NL,10,-1,5,50,0,0,0,0,0
LineSpacing=0
TabColor=255,255,255

[Cursor Options]
CursorShape=2
CustomCursorColor=255,255,255
UseCustomCursorColor=true

[General]
Command=/usr/bin/tmux
Icon=xfce-terminal
LocalTabTitleFormat=%n %#
Name=blue_default
Parent=FALLBACK/
RemoteTabTitleFormat=(%u) %H
TerminalCenter=true
TerminalColumns=100
TerminalMargin=3
TerminalRows=28

[Scrolling]
ScrollBarPosition=2

[Terminal Features]
BlinkingCursorEnabled=true
